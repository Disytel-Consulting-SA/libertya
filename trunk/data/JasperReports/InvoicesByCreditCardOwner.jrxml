	<parameter name="DateFrom" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="DateTo" isForPrompting="false" class="java.sql.Timestamp"/>
	<queryString><![CDATA[select o.value as orgvalue, o.name as orgname, i.c_invoice_id, i.documentno, i.dateinvoiced, (CASE WHEN $P{C_BPartner_ID} is null OR $P{C_BPartner_ID} = 0 then p.a_name ELSE bp.name END) as titular, ef.name as tarjeta, efp.cuotaspago, i.grandtotal, il.line, pro.value as productvalue, pro.name as productname, il.qtyinvoiced, il.priceactual
from c_payment as p
inner join c_bpartner as bp on bp.c_bpartner_id = p.c_bpartner_id
inner join m_entidadfinancieraplan as efp on efp.m_entidadfinancieraplan_id = p.m_entidadfinancieraplan_id
inner join m_entidadfinanciera as ef on ef.m_entidadfinanciera_id = efp.m_entidadfinanciera_id
inner join ad_org as o on o.ad_org_id = p.ad_org_id
inner join c_allocationline as al on al.c_payment_id = p.c_payment_id
inner join c_invoice as i on i.c_invoice_id = al.c_invoice_id
inner join c_doctype as dt on dt.c_doctype_id = i.c_doctypetarget_id
inner join c_invoiceline as il on il.c_invoice_id = i.c_invoice_id
inner join m_product as pro on pro.m_product_id = il.m_product_id
where p.tendertype = 'C' 
	AND p.ad_org_id = $P{AD_Org_ID} 
	AND p.docstatus IN ('CO','CL')
	AND dt.docbasetype = 'ARI'
	AND (CASE WHEN $P{C_BPartner_ID} is null OR $P{C_BPartner_ID} = 0 THEN upper(p.a_name) like upper($P{CreditCardOwner})
		ELSE $P{C_BPartner_ID} = p.c_bpartner_id END)
	AND ($P{DateFrom}::date is null OR $P{DateFrom}::date <= p.datetrx::date)
	AND ($P{DateTo}::date is null OR p.datetrx::date <= $P{DateTo}::date)
order by (CASE WHEN $P{C_BPartner_ID} is null OR $P{C_BPartner_ID} = 0 THEN p.a_name ELSE bp.name END), i.dateinvoiced, i.documentno, il.line]]></queryString>

