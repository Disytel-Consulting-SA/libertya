	<parameter name="AD_CLIENT_ID" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="C_BankAccount_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="DateFrom" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="DateTo" isForPrompting="false" class="java.sql.Timestamp"/>
	<queryString><![CDATA[select bs.c_bankstatement_id, bs.name as bankstatement_name, bs.statementdate, ba.description as bankaccount_description, 
	bsl.c_bankstatementline_id, coalesce(bsl.description,p.description) as bankstatementline_description, bsl.stmtamt, bsl.line, 
	p.c_payment_id, dt.c_doctype_id, dt.name as payment_doctypename
from c_bankstatement bs
inner join c_bankaccount ba on ba.c_bankaccount_id = bs.c_bankaccount_id
inner join c_bankstatementline bsl on bsl.c_bankstatement_id = bs.c_bankstatement_id
left join c_payment p on bsl.c_payment_id = p.c_payment_id
left join c_doctype dt on dt.c_doctype_id = p.c_doctype_id
where bs.ad_client_id = $P{AD_CLIENT_ID}
	and ($P{C_BankAccount_ID} is null OR $P{C_BankAccount_ID} = 0 OR bs.c_bankaccount_id = $P{C_BankAccount_ID})
	and ($P{DateFrom}::date is null OR bs.statementdate >= $P{DateFrom}::date)
	and ($P{DateTo}::date is null OR bs.statementdate <= $P{DateTo}::date)
order by bs.statementdate, bs.name, bs.c_bankstatement_id, bsl.line]]></queryString>

