	<parameter name="M_PriceList_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<queryString><![CDATA[select *
from 
(select *,
	(case when coalesce(price2, 0) = 0 then 100 else ((price2/price1) - 1) * 100 end)::numeric(20,4) as diff_porc,
	coalesce(price2, 0) - price1 as diff_amt
from 
(select p.m_product_id, p.value, p.name, u.x12de355, pp1.pricestd as price1, pp2.pricestd as price2
from m_productprice pp1
join m_product p on p.m_product_id = pp1.m_product_id
join c_uom u on u.c_uom_id = p.c_uom_id
left join m_productprice pp2 on pp1.m_product_id = pp2.m_product_id
where pp1.m_pricelist_version_id = $P{M_PriceList_Version_From_ID} and pp2.m_pricelist_version_id = $P{M_PriceList_Version_To_ID}
	and ($P{C_BPartner_ID} is null or $P{C_BPartner_ID} = 0 or pp1.m_product_id in (select m_product_id from m_product_po where c_bpartner_id = $P{C_BPartner_ID}))) as p
where price2 is null or price1 <> price2) as t
order by value]]></queryString>

