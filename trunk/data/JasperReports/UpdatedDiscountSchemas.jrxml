	<queryString><![CDATA[select ds.ad_client_id, 
	ds.ad_org_id, 
	ds.name as discountschema_name, 
	dsl.name as discountschemaline_name,
	dsl.seqno,
	dsl.updated,
	u.name as user_name,
	u.description as user_description
from m_discountschema as ds
inner join m_discountschemaline as dsl on dsl.m_discountschema_id = ds.m_discountschema_id
inner join ad_user as u on u.ad_user_id = dsl.updatedby
where ds.ad_client_id = $P{AD_CLIENT_ID} 
	and ds.discounttype = 'P' 
	and (dsl.updated::date >= $P{Date_From}::date)
	and (dsl.updated::date <= $P{Date_To}::date)
	and ($P{M_DiscountSchema_ID} is null or $P{M_DiscountSchema_ID} = 0 or dsl.m_discountschema_id = $P{M_DiscountSchema_ID})
	and ($P{AD_User_ID} is null or dsl.updatedby = $P{AD_User_ID})
order by ds.name, dsl.seqno]]></queryString>

