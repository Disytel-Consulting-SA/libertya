	</parameter>
	<parameter name="AD_CLIENT_ID" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="FechaHasta" isForPrompting="false" class="java.util.Date">
		<defaultValueExpression ><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="grupo_ec" isForPrompting="false" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="M_Product_Category_ID" isForPrompting="false" class="java.math.BigDecimal">
		<defaultValueExpression ><![CDATA[BigDecimal.ZERO]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT 
ad_org_id,
organization,
group_ec,
group_ec_value,
fecha,
tipo_de_documento,
nro_de_documento,
entidad_comercial,
importe_neto,
impuestos,
percepciones, 
importe_neto + impuestos + percepciones as importe_total,
articulo,
subfamilia,
region
FROM (
SELECT 
i.ad_org_id,
ado.value as organization,
bp.c_bp_group_id as group_ec,
bpg.value as group_ec_value,
i.dateinvoiced as fecha,
dt.name as tipo_de_documento,
i.documentno as nro_de_documento,
bp.value as entidad_comercial,
i.grandtotal - i.totallines as importe_neto,
il.SumaTaxAmt as impuestos,
case when i.totallines > 0 then (((iline.linetotalamt / i.totallines)*(it.taxamt))+iline.linetotalamt)
else 0 
end as percepciones,
p.name as articulo,
pc.value as subfamilia,
pc.m_product_category_id as subfamiliaID,
r.name as region
FROM c_invoice i
INNER JOIN (select il.c_invoice_id, SUM(taxamt) as SumaTaxAmt from c_invoiceline il group by il.c_invoice_id) as il ON (il.c_invoice_id = i.c_invoice_id)
INNER JOIN c_invoiceline iline ON (iline.c_invoice_id = i.c_invoice_id)
INNER JOIN m_product p ON (p.m_product_id = iline.m_product_id)
INNER JOIN m_product_category pc ON (pc.m_product_category_id = p.m_product_category_id)
INNER JOIN c_invoicetax it ON (i.c_invoice_id = it.c_invoice_id)
INNER JOIN c_bpartner bp ON (bp.c_bpartner_id = i.c_bpartner_id)
INNER JOIN c_bp_group bpg ON (bpg.c_bp_group_id = bp.c_bp_group_id)
INNER JOIN c_location l ON (l.c_location_id = bp.c_location_id)
INNER JOIN c_region r ON (r.c_region_id = l.c_region_id)
INNER JOIN c_doctype dt ON (dt.c_doctype_id = i.c_doctype_id)
INNER JOIN ad_org ado ON (ado.ad_org_id = i.ad_org_id)
WHERE (i.issotrx = 'N') 
AND (dt.docbasetype in ('APC','API'))
order by i.c_invoice_id) AS VentasPorRegion
WHERE (ad_org_id = $P{AD_Org_ID})
AND (fecha BETWEEN $P{FechaDesde} AND $P{FechaHasta}) 
AND (($P{grupo_ec} IS NULL) OR (group_ec = $P{grupo_ec}))
AND (($P{M_Product_Category_ID} IS NULL) OR (subfamiliaID = $P{M_Product_Category_ID}))]]></queryString>

