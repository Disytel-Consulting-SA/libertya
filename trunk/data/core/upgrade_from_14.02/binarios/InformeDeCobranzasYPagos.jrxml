	<queryString><![CDATA[SELECT
c_allocationhdr_id,
organization,
grupo_ec,
fecha,
entidadcomercial,
nrorecibo,
tipoderecibo,
tipo,
monto
FROM((select
a.c_allocationhdr_id,
a.ad_org_id,
bp.c_bp_group_id,
o.name as organization,
bpg.name as grupo_ec,
a.datetrx as Fecha,
bp.c_bpartner_id as EntidadComercialID,
bp.name as EntidadComercial,
a.documentno as NroRecibo,
dt.name as TipoDeRecibo,
rs.name as Tipo,
mri.amt_retenc as Monto
from c_allocationhdr a
inner join c_allocationline al on (al.c_allocationhdr_id = a.c_allocationhdr_id)
inner join ad_org o on (o.ad_org_id = a.ad_org_id)
inner join c_bpartner bp on (bp.c_bpartner_id = a.c_bpartner_id)
inner join c_bp_group bpg on (bpg.c_bp_group_id = bp.c_bp_group_id)
inner join c_doctype dt on (dt.c_doctype_id = a.c_doctype_id)
left join m_retencion_invoice mri on (mri.c_allocationhdr_id = a.c_allocationhdr_id)
left join c_retencionschema rs on (rs.c_retencionschema_id = mri.c_retencionschema_id)
where docstatus in ('CO')
AND (case $P{Mostrar} when (cast ('P' as varchar)) then dt.docbasetype in ('APP') 
else dt.docbasetype in ('ARR') end)
AND (($P{grupoec} IS NULL) OR (bpg.c_bp_group_id = $P{grupoec}))
AND (($P{EntidadComercial} IS NULL) OR (bp.c_bpartner_id = $P{EntidadComercial}))
)
UNION
(select
a.c_allocationhdr_id,
a.ad_org_id,
bp.c_bp_group_id,
o.name as organization,
bpg.name as grupo_ec,
a.datetrx as Fecha,
bp.c_bpartner_id as EntidadComercialID,
bp.name as EntidadComercial,
a.documentno as NroRecibo,
dt.name as TipoDeRecibo,
'Cheque' as Tipo,
al.amount as Monto
from c_allocationhdr a
inner join c_allocationline al on (al.c_allocationhdr_id = a.c_allocationhdr_id)
inner join ad_org o on (o.ad_org_id = a.ad_org_id)
inner join c_bpartner bp on (bp.c_bpartner_id = a.c_bpartner_id)
inner join c_bp_group bpg on (bpg.c_bp_group_id = bp.c_bp_group_id)
inner join c_doctype dt on (dt.c_doctype_id = a.c_doctype_id)
inner join c_payment p on (p.c_payment_id = al.c_payment_id)
where (al.c_allocationhdr_id = a.c_allocationhdr_id) and (p.tendertype in ('K'))
AND (case $P{Mostrar} when (cast ('P' as varchar)) then dt.docbasetype in ('APP') 
else dt.docbasetype in ('ARR') end)
AND (($P{grupoec} IS NULL) OR (bpg.c_bp_group_id = $P{grupoec}))
AND (($P{EntidadComercial} IS NULL) OR (bp.c_bpartner_id = $P{EntidadComercial}))
)
UNION
(select
a.c_allocationhdr_id,
a.ad_org_id,
bp.c_bp_group_id,
o.name as organization,
bpg.name as grupo_ec,
a.datetrx as Fecha,
bp.c_bpartner_id as EntidadComercialID,
bp.name as EntidadComercial,
a.documentno as NroRecibo,
dt.name as TipoDeRecibo,
'Transferencia Bancaria' as Tipo,
al.amount as Monto
from c_allocationhdr a
inner join c_allocationline al on (al.c_allocationhdr_id = a.c_allocationhdr_id)
inner join ad_org o on (o.ad_org_id = a.ad_org_id)
inner join c_bpartner bp on (bp.c_bpartner_id = a.c_bpartner_id)
inner join c_bp_group bpg on (bpg.c_bp_group_id = bp.c_bp_group_id)
inner join c_doctype dt on (dt.c_doctype_id = a.c_doctype_id)
inner join c_payment p on (p.c_payment_id = al.c_payment_id)
where (al.c_allocationhdr_id = a.c_allocationhdr_id) and (p.tendertype in ('A'))
AND (case $P{Mostrar} when (cast ('P' as varchar)) then dt.docbasetype in ('APP') 
else dt.docbasetype in ('ARR') end)
AND (($P{grupoec} IS NULL) OR (bpg.c_bp_group_id = $P{grupoec}))
AND (($P{EntidadComercial} IS NULL) OR (bp.c_bpartner_id = $P{EntidadComercial}))
)
UNION
(select
a.c_allocationhdr_id,
a.ad_org_id,
bp.c_bp_group_id,
o.name as organization,
bpg.name as grupo_ec,
a.datetrx as Fecha,
bp.c_bpartner_id as EntidadComercialID,
bp.name as EntidadComercial,
a.documentno as NroRecibo,
dt.name as TipoDeRecibo,
'Caja' as Tipo,
al.amount as Monto
from c_allocationhdr a
inner join c_allocationline al on (al.c_allocationhdr_id = a.c_allocationhdr_id)
inner join ad_org o on (o.ad_org_id = a.ad_org_id)
inner join c_bpartner bp on (bp.c_bpartner_id = a.c_bpartner_id)
inner join c_bp_group bpg on (bpg.c_bp_group_id = bp.c_bp_group_id)
inner join c_doctype dt on (dt.c_doctype_id = a.c_doctype_id)
inner join c_cashline c on (c.c_cashline_id = al.c_cashline_id)
where (al.c_allocationhdr_id = a.c_allocationhdr_id)
AND (case $P{Mostrar} when (cast ('P' as varchar)) then dt.docbasetype in ('APP') 
else dt.docbasetype in ('ARR') end)
AND (($P{grupoec} IS NULL) OR (bpg.c_bp_group_id = $P{grupoec}))
AND (($P{EntidadComercial} IS NULL) OR (bp.c_bpartner_id = $P{EntidadComercial}))
)
UNION
(select
al.c_allocationhdr_id,
null as ad_org_id,
null as c_bp_group_id,
null as organization,
null as grupo_ec,
null as Fecha,
null as EntidadComercialID,
null as EntidadComercial,
null as NroRecibo,
null as TipoDeRecibo,
'Diferencia de Caja' as Tipo,
al.amount - allo.amount as Monto
from c_invoice i
inner join c_invoiceline il on (il.c_invoice_id = i.c_invoice_id)
left join c_allocationline al on (al.c_invoice_id = i.c_invoice_id)
left join c_allocationline allo on (allo.c_invoice_credit_id = i.c_invoice_id)
where il.m_product_id = 
cast((select value from ad_preference where attribute = 'DIF_CAMBIO_ARTICULO') as INTEGER)
)
UNION
(select
al.c_allocationhdr_id,
null as ad_org_id,
null as c_bp_group_id,
null as organization,
null as grupo_ec,
null as Fecha,
null as EntidadComercialID,
null as EntidadComercial,
null as NroRecibo,
null as TipoDeRecibo,
'Nota de Credito' as Tipo,
i.grandtotal as Monto
from c_invoice i
inner join c_invoiceline il on (il.c_invoice_id = i.c_invoice_id)
inner join c_allocationline al on (al.c_invoice_credit_id = i.c_invoice_id)
where il.m_product_id <> 
cast((select value from ad_preference where attribute = 'DIF_CAMBIO_ARTICULO') as INTEGER)
and (i.c_invoice_id not in (select c_invoice_id from m_retencion_invoice))
) 
UNION
(select
a.c_allocationhdr_id,
a.ad_org_id,
bp.c_bp_group_id,
o.name as organization,
bpg.name as grupo_ec,
a.datetrx as Fecha,
bp.c_bpartner_id as EntidadComercialID,
bp.name as EntidadComercial,
a.documentno as NroRecibo,
dt.name as TipoDeRecibo,
'Transferencia Bancaria' as Tipo,
al.amount as Monto
from c_allocationhdr a
inner join c_allocationline al on (al.c_allocationhdr_id = a.c_allocationhdr_id)
inner join ad_org o on (o.ad_org_id = a.ad_org_id)
inner join c_bpartner bp on (bp.c_bpartner_id = a.c_bpartner_id)
inner join c_bp_group bpg on (bpg.c_bp_group_id = bp.c_bp_group_id)
inner join c_doctype dt on (dt.c_doctype_id = a.c_doctype_id)
inner join c_payment p on (p.c_payment_id = al.c_payment_id)
where (al.c_allocationhdr_id = a.c_allocationhdr_id) and (p.tendertype in ('C'))
AND (case $P{Mostrar} when (cast ('P' as varchar)) then dt.docbasetype in ('APP') 
else dt.docbasetype in ('ARR') end)
AND (($P{grupoec} IS NULL) OR (bpg.c_bp_group_id = $P{grupoec}))
AND (($P{EntidadComercial} IS NULL) OR (bp.c_bpartner_id = $P{EntidadComercial}))
)
UNION
(select
al.c_allocationhdr_id,
null as ad_org_id,
null as c_bp_group_id,
null as organization,
null as grupo_ec,
null as Fecha,
null as EntidadComercialID,
null as EntidadComercial,
null as NroRecibo,
null as TipoDeRecibo,
'Otro' as Tipo,
i.grandtotal as Monto
from c_invoice i
inner join c_allocationline al on (al.c_invoice_credit_id = i.c_invoice_id)
inner join c_allocationhdr a on (a.c_allocationhdr_id = al.c_allocationhdr_id) 
inner join c_doctype dt on (dt.c_doctype_id = a.c_doctype_id)
where (i.issotrx <> dt.issotrx)
AND (case $P{Mostrar} when (cast ('P' as varchar)) then dt.docbasetype in ('APP') 
else dt.docbasetype in ('ARR') end)
)) as subconsulta
WHERE (($P{AD_Org_ID} IS NULL) OR (ad_org_id = $P{AD_Org_ID}))
AND (($P{FechaDesde} IS NULL) OR (Fecha BETWEEN $P{FechaDesde} AND $P{FechaHasta}))
ORDER BY c_allocationhdr_id]]></queryString>

