	<queryString><![CDATA[select o.value orgvalue, o.name orgname, p.name posname, p.posnumber, u.name username, u.description userdescription, pj.datetrx 
from c_posjournal pj
inner join c_pos p on p.c_pos_id = pj.c_pos_id
inner join ad_user u on u.ad_user_id = pj.ad_user_id
inner join ad_org o on o.ad_org_id = pj.ad_org_id
where o.ad_org_id = $P{AD_Org_ID}
	AND date_trunc('day',pj.datetrx) >= date_trunc('day',$P{DateFrom}::date) 
	AND date_trunc('day',pj.datetrx) <= date_trunc('day',$P{DateTo}::date)
order by o.value, datetrx, p.posnumber, u.name]]></queryString>

