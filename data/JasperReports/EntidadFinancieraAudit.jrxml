	<parameter name="AD_Org_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="M_EntidadFinanciera_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="C_BPartner_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="EntidadFinancieraName" isForPrompting="false" class="java.lang.String"/>
	<parameter name="EntidadFinancieraValue" isForPrompting="false" class="java.lang.String"/>
	<parameter name="Date" isForPrompting="false" class="java.sql.Timestamp">
		<defaultValueExpression ><![CDATA[new java.sql.Timestamp(System.currentTimeMillis())]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT org_value, 
	org_name, 
	c_bpartner_id,
	bpartnerValue, 
	bpartnerName, 
	couponbatchnumber, 
	coalesce(sum(sales),0) as sales,
	coalesce(sum(purchases),0) as purchases
FROM (SELECT o.value as org_value,
	o.name as org_name,
	bp.c_bpartner_id,
	bp.value as bpartnerValue, 
	bp.name as bpartnerName, 
	coalesce(couponbatchnumber,'SIN NRO DE LOTE') as couponbatchnumber, 
	(CASE WHEN p.isreceipt = 'Y' THEN currencybase(p.payamt, p.c_currency_id, p.datetrx, p.ad_client_id, p.ad_org_id) ELSE 0.00 END) as sales,
	(CASE WHEN p.isreceipt = 'Y' THEN 0.00 ELSE currencybase(p.payamt, p.c_currency_id, p.datetrx, p.ad_client_id, p.ad_org_id) END) as purchases
FROM c_payment p 
INNER JOIN m_entidadfinancieraplan efp ON efp.m_entidadfinancieraplan_id = p.m_entidadfinancieraplan_id
INNER JOIN m_entidadfinanciera ef ON ef.m_entidadfinanciera_id = efp.m_entidadfinanciera_id
INNER JOIN c_bpartner as bp ON bp.c_bpartner_id = ef.c_bpartner_id
INNER JOIN ad_org o ON o.ad_org_id = p.ad_org_id
WHERE (p.ad_org_id = $P{AD_Org_ID}) 
	AND p.docstatus IN ('CO','CL') 
	AND date_trunc('day',p.datetrx) = date_trunc('day',$P{Date}::date)
	AND ((($P{C_BPartner_ID} is null or $P{C_BPartner_ID} = 0) 
			and ($P{M_EntidadFinanciera_ID} is null or $P{M_EntidadFinanciera_ID} = 0))
		OR  (case when ($P{C_BPartner_ID} is null or $P{C_BPartner_ID} = 0) then ef.m_entidadfinanciera_id = $P{M_EntidadFinanciera_ID} else bp.c_bpartner_id = $P{C_BPartner_ID} end))) p
GROUP BY org_value, org_name, c_bpartner_id, bpartnerValue, bpartnerName, couponbatchnumber
ORDER BY org_value, bpartnerValue, couponbatchnumber]]></queryString>

