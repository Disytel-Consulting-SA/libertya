	<parameter name="AD_Org_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="DateFrom" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="DateTo" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="AD_CLIENT_ID" isForPrompting="false" class="java.lang.Integer"/>
	<queryString><![CDATA[select ad_org_id, orgvalue, orgname, c_invoice_id, datecomp, doctypename, 
	signo_issotrx, documentno, puntodeventa, nrocomprobante, 
	nombrecomprador, cae, vtocae, nroidentificacioncomprador, letra, 
	categoriaiva, netamount * signo_issotrx as netamount, 
	grandtotal * signo_issotrx as grandtotal, 
	sum(iva * signo_issotrx) as iva,
	sum(otrosimp * signo_issotrx) as otrosimp
from 
(select i.ad_org_id, o.value as orgvalue, o.name as orgname, i.c_invoice_id, ri.date::date as datecomp, 
	dt.name as doctypename, dt.signo_issotrx, i.documentno, ri.puntodeventa, ri.nrocomprobante, 
	ri.nombrecomprador, i.cae, i.vtocae, ri.nroidentificacioncomprador, lc.letra, 
	ci.name as categoriaiva, i.netamount, i.grandtotal, 
	(CASE WHEN tc.ismanual = 'N' THEN it.taxamt ELSE 0 END) as iva,
	(CASE WHEN tc.ismanual = 'Y' THEN it.taxamt ELSE 0 END) as otrosimp
from reginfo_ventas_cbte_v  ri
join c_invoice i on i.c_invoice_id = ri.c_invoice_id
join c_doctype dt on dt.c_doctype_id = i.c_doctypetarget_id
join ad_org o on o.ad_org_id = i.ad_org_id
join c_bpartner bp on bp.c_bpartner_id = i.c_bpartner_id
join c_categoria_iva ci on ci.c_categoria_iva_id = bp.c_categoria_iva_id
left join c_letra_comprobante lc on lc.c_letra_comprobante_id = i.c_letra_comprobante_id
join c_invoicetax it on it.c_invoice_id = i.c_invoice_id
join c_tax t on t.c_tax_id = it.c_tax_id
join c_taxcategory tc on t.c_taxcategory_id = tc.c_taxcategory_id
left join c_region r on r.c_region_id = t.c_region_id
where ri.ad_client_id = $P{AD_CLIENT_ID}
	and ($P{AD_Org_ID} is null or $P{AD_Org_ID} = 0 or ri.ad_org_id = $P{AD_Org_ID})
	and (ri.date::date between $P{DateFrom}::date and $P{DateTo}::date)
	and dt.doctypekey::text <> ALL (ARRAY['RTR'::character varying, 'RTI'::character varying, 'RCR'::character varying, 'RCI'::character varying]::text[])
	AND ((dt.isfiscaldocument = 'Y') 
		AND (dt.isfiscal is null OR dt.isfiscal = 'N' OR (dt.isfiscal = 'Y' AND i.fiscalalreadyprinted = 'Y')) 
		AND (dt.iselectronic is null OR dt.iselectronic = 'N' OR (dt.iselectronic = 'Y' AND i.cae is not null)))
	AND i.docstatus IN ('CO','CL','RE','VO')
	AND ((i.issotrx = 'Y' AND dt.transactiontypefrontliva is null) OR dt.transactiontypefrontliva = 'S')
) as i
group by ad_org_id, orgvalue, orgname, c_invoice_id, datecomp, doctypename, 
	signo_issotrx, documentno, puntodeventa, nrocomprobante, 
	nombrecomprador, cae, vtocae, nroidentificacioncomprador, letra, 
	categoriaiva, netamount, grandtotal
order by datecomp, doctypename, puntodeventa, nrocomprobante]]></queryString>

