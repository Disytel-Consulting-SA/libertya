	<parameter name="M_DiscountSchema_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="AD_CLIENT_ID" isForPrompting="false" class="java.lang.Integer"/>
	<queryString><![CDATA[select o.ad_org_id, o.value as orgvalue, o.name as orgname, i.c_doctypetarget_id, dt.name as doctypename, 
	i.documentno, i.dateacct, dd.m_discountschema_id, ds.name as discountschemaname, 
	dd.description as discountdescription, dd.created, dd.taxrate, dd.discountapplication, dd.discountkind, 
	dd.discountbaseamt, abs(dd.discountamt) * signo_issotrx as discountamt
from c_documentdiscount dd
join c_invoice i on i.c_invoice_id = dd.c_invoice_id
join c_doctype dt on dt.c_doctype_id = i.c_doctypetarget_id
join ad_org o on o.ad_org_id = i.ad_org_id
left join m_discountschema ds on ds.m_discountschema_id = dd.m_discountschema_id
where i.ad_client_id = $P{AD_CLIENT_ID} and dd.discountamt < 0 
	and dd.taxrate is not null and i.docstatus in ('CO','CL') 
	AND dateacct::date >= $P{DateFrom}::date
	AND dateacct::date <= $P{DateTo}::date
	AND ($P{AD_Org_ID} is null OR $P{AD_Org_ID} = 0 OR $P{AD_Org_ID} = i.ad_org_id)
	AND ($P{M_DiscountSchema_ID} is null OR $P{M_DiscountSchema_ID} = 0 OR $P{M_DiscountSchema_ID} = dd.m_discountschema_id)
order by o.value, dd.created]]></queryString>

