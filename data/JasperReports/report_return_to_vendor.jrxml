	<parameter name="AD_Org_ID" isForPrompting="true" class="java.math.BigDecimal">
		<defaultValueExpression ><![CDATA[$P{AD_Org_ID}]]></defaultValueExpression>
	</parameter>
	<parameter name="C_BPartner_ID" isForPrompting="true" class="java.math.BigDecimal"/>
	<parameter name="estado" isForPrompting="true" class="java.lang.String"/>
	<parameter name="FechaDesde" isForPrompting="true" class="java.util.Date"/>
	<parameter name="FechaHasta" isForPrompting="true" class="java.util.Date"/>
	<queryString><![CDATA[SELECT 	ad_org.name	 		as organizacion,
	m_inout.movementdate 		as fecha, 
	m_inout.documentno 		as nro_devolucion, 
	c_bpartner.name 		as proveedor, 
	m_inout.description 		as descripcion,
	text ''				as estado,
	c_invoice.dateinvoiced 		as fecha_nc, 
	c_invoice.numerodedocumento 	as nro_nc, 
	c_invoice.grandtotal 		as importe_nc
FROM m_inout
INNER JOIN ad_org ON ad_org.ad_org_id = m_inout.ad_org_id
INNER JOIN c_bpartner ON m_inout.c_bpartner_id = c_bpartner.c_bpartner_id
LEFT JOIN c_invoice ON m_inout.c_invoice_id = c_invoice.c_invoice_id
WHERE m_inout.c_doctype_id IN (SELECT c_doctype_id FROM c_doctype WHERE (docbasetype = 'MMR' AND doctypekey = 'VR'))
AND (m_inout.docstatus IN ('CO','CL'))

AND (($P{AD_Org_ID} IS NULL) OR (m_inout.AD_Org_ID = $P{AD_Org_ID})) 
AND (($P{C_BPartner_ID} IS NULL) OR (m_inout.c_bpartner_id = $P{C_BPartner_ID}))

AND (($P{FechaDesde}::date IS NULL) OR (m_inout.movementdate::date >= $P{FechaDesde}::date)) 
AND (($P{FechaHasta}::date IS NULL) OR (m_inout.movementdate::date <= $P{FechaHasta}::date))

AND (
	CASE WHEN ($P{estado} = 'WithNC') THEN (m_inout.c_invoice_id IS NOT NULL) 
	WHEN ($P{estado} = 'WithOutNC') THEN (m_inout.c_invoice_id IS NULL) 
	WHEN ($P{estado} = 'ShowAll') THEN ((m_inout.c_invoice_id IS NOT NULL) OR (m_inout.c_invoice_id IS NULL))
END)
ORDER BY m_inout.movementdate]]></queryString>

