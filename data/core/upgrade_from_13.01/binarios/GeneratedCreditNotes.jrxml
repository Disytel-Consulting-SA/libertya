	<parameter name="AD_Org_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="DateFrom" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="DateTo" isForPrompting="false" class="java.sql.Timestamp"/>
	<queryString><![CDATA[select c_invoice_id, 
	documentno, 
	orgvalue,
	orgname,
	c_bpartner_id, 
	nombrecli, 
	responsable, 
	cond_vta, 
	dateinvoiced, 
	grandtotal 
from (select i.c_invoice_id, 
		i.documentno, 
		o.value as orgvalue,
		o.name as orgname,
		i.c_bpartner_id, 
		coalesce(i.nombrecli, bp.value || ' ' || bp.name) as nombrecli, 
		(CASE WHEN u.description is not null AND length(trim(u.description)) > 0 THEN u.description ELSE u.name END) as responsable, 
		grandtotal, 
		i.dateinvoiced, 
		(SELECT payment_medium_name 
			FROM c_allocation_detail_v as ad 
			WHERE ad.c_invoice_id = i.c_invoice_id LIMIT 1) as cond_vta 
	from c_invoice as i 
	inner join ad_org as o on o.ad_org_id = i.ad_org_id
	inner join c_bpartner as bp on bp.c_bpartner_id = i.c_bpartner_id 
	inner join c_doctype as dt on dt.c_doctype_id = i.c_doctypetarget_id 
	inner join ad_user as u on u.ad_user_id = i.salesrep_id
	where i.ad_org_id = $P{AD_Org_ID}
		AND dt.docbasetype = 'ARC' 
		AND dt.doctypekey NOT IN ('RCR') 
		AND date_trunc('day',i.dateinvoiced) >= date_trunc('day',$P{DateFrom}::date) 
		AND date_trunc('day',i.dateinvoiced) <= date_trunc('day',$P{DateTo}::date)
		AND (i.docstatus IN ('CL','CO') OR (i.docstatus IN ('VO','RE') AND (dt.isfiscal is null OR dt.isfiscal = 'N' OR (dt.isfiscal = 'Y' AND i.fiscalalreadyprinted = 'Y'))))) as d 
order by documentno]]></queryString>

