	<parameter name="AD_Org_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="FechaDesde" isForPrompting="false" class="java.util.Date"/>
	<parameter name="AD_CLIENT_ID" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="C_BPartner_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="FechaHasta" isForPrompting="false" class="java.util.Date"/>
	<parameter name="AD_User_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="StatusAuthorization" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT sub.organizacion,sub.fechaFactura, sub.documentno, sub.codprov, sub.bpartner,autorizada, sub.grandtotal, sub.netamount, sub.balance,
	sub.usuario, sub.proximousuario
FROM
(SELECT org.name as organizacion, 
i.dateinvoiced as fechaFactura, 
i.documentno,
invoiceopen(i.c_invoice_id,0) as balance,
bp.value as codprov,
bp.name bpartner, 
i.c_invoice_id,
(CASE WHEN (SELECT COUNT(*) 
		FROM M_AuthorizationChainDocument acd 
		WHERE acd.C_Invoice_ID = i.C_Invoice_ID AND Status = 'P') > 0 THEN 'N' ELSE 'Y' END) As Autorizada,
i.grandtotal,
i.netamount,
(SELECT u.name FROM AD_User u
	INNER JOIN M_AuthorizationChainDocument acd ON (acd.AD_User_ID = u.AD_User_ID) 
	INNER JOIN M_AuthorizationChainLink acl ON (acd.M_AuthorizationChainLink_ID = acl.M_AuthorizationChainLink_ID)
	WHERE acd.C_Invoice_ID = i.C_Invoice_ID
	ORDER BY acl.linknumber DESC
	LIMIT 1) as Usuario,
acd.proximousuario
FROM C_Invoice i
INNER JOIN AD_Org org ON (org.AD_Org_ID = i.AD_Org_ID)
INNER JOIN AD_Client cli ON (cli.AD_Client_ID = i.AD_Client_ID)
INNER JOIN C_Bpartner bp ON (bp.C_BPartner_ID = i.C_Bpartner_ID)
INNER JOIN C_Doctype dt ON (dt.C_Doctype_ID = i.C_DocType_ID)
LEFT JOIN (SELECT array_agg (u.name) as proximoUsuario, array_agg (u.AD_User_ID) as proximoUsuarioID,acd.C_Invoice_ID
	FROM M_AuthorizationChainLink acl
	INNER JOIN M_AuthorizationChainLinkUser aclu ON (aclu.M_AuthorizationChainLink_ID = acl.M_AuthorizationChainLink_ID)
	INNER JOIN M_AuthorizationChainDocument acd ON (acd.M_AuthorizationChainLink_ID = acl.M_AuthorizationChainLink_ID)
	INNER JOIN AD_User u ON (u.AD_User_ID = aclu.AD_User_ID)
	WHERE Status = 'P'
	AND (case when aclu.StartDate is null then true else current_date >= aclu.EndDate end) 
	AND (case when aclu.EndDate is null then true else current_date <= aclu.EndDate end)
	AND (($P{AD_User_ID} IS NULL) OR (u.AD_User_ID = $P{AD_User_ID}))
	GROUP BY acd.C_Invoice_ID
	) acd ON (acd.C_Invoice_ID = i.C_Invoice_ID)
WHERE (($P{AD_Org_ID} IS NULL) OR (0 = $P{AD_Org_ID}) OR (i.ad_org_id = $P{AD_Org_ID}))
	AND (($P{AD_CLIENT_ID} IS NULL) OR (i.ad_client_id = $P{AD_CLIENT_ID}))
	AND invoiceopen(i.c_invoice_id,0) > 0
	AND i.docstatus in ('CL','CO','WC')
	AND dt.DocBaseType IN ('API')
	AND (($P{C_BPartner_ID} IS NULL) OR (bp.c_bpartner_id = $P{C_BPartner_ID}))
	AND (($P{FechaDesde}::date IS NULL) OR (i.dateinvoiced::date >= $P{FechaDesde}::date)) 
	AND (($P{FechaHasta}::date IS NULL) OR (i.dateinvoiced::date <= $P{FechaHasta}::date)) 
	AND (($P{AD_User_ID} IS NULL) OR (select array_agg($P{AD_User_ID}::int) <@ acd.proximousuarioID))
ORDER BY dateinvoiced DESC) as Sub
WHERE (CASE WHEN $P{StatusAuthorization} = 'P' THEN Autorizada='N' ELSE Autorizada='Y' END)]]></queryString>

