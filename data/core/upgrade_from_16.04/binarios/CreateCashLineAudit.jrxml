	<parameter name="DateFrom" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="DateTo" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="AD_User_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="AD_Org_Login_ID" isForPrompting="false" class="java.lang.Integer"/>
	<queryString><![CDATA[select *
from (select ad_pinstance_id,
	created,
	ad_user_id, 
	username,
	max(ad_org_id) as ad_org_id,
	max(org_value) as org_value,
	max(org_name) as org_name,
	max(cash_name) as cash_name,
	max(cash_date) as cash_date,
	max(charge_name) as charge_name,
	max(amount) as amount
from (select p.ad_pinstance_id,
	p.created, 
	p.ad_user_id,
	p.username,
	parametername,
	o.ad_org_id,
	o.value as org_value,
	o.name as org_name,
	c.name as cash_name,
	c.statementdate as cash_date,
	ch.name as charge_name,
	(CASE WHEN parametername = 'Amount' THEN parameter_value_number ELSE null END) as amount
from (select p.ad_pinstance_id, p.ad_org_id, p.created, u.ad_user_id, u.name as username, parametername, p_string as parameter_value_string, (CASE WHEN position('_ID' in parametername) > 0 THEN trunc(p_number) ELSE p_number END) as parameter_value_number
	from ad_pinstance as p
	inner join ad_pinstance_para as pp on pp.ad_pinstance_id = p.ad_pinstance_id
	inner join ad_user as u on u.ad_user_id = p.ad_user_id
	inner join ad_process as pro on pro.ad_process_id = p.ad_process_id
	where pro.ad_componentobjectuid = 'CORE-AD_Process-1010499') as p
left join c_cash as c on (p.parametername = 'C_Cash_ID' AND c.c_cash_id = p.parameter_value_number)
left join ad_org as o on o.ad_org_id = c.ad_org_id
left join ad_user as u on u.ad_user_id = c.createdby
left join c_charge as ch on (p.parametername = 'C_Charge_ID' AND ch.c_charge_id = p.parameter_value_number)) as a
group by ad_pinstance_id,
	created,
	ad_user_id, 
	username
order by created, ad_pinstance_id) as e
where amount <> 0 
	and ad_org_id = $P{AD_Org_Login_ID}
	and ($P{DateFrom}::date is null OR created::date >= $P{DateFrom}::date)
	and ($P{DateTo}::date is null OR created::date <= $P{DateTo}::date)
	and ($P{AD_User_ID} is null OR $P{AD_User_ID} = 0 OR $P{AD_User_ID} = ad_user_id)	]]></queryString>

