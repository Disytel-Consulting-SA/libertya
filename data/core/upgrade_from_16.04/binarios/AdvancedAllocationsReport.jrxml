	<parameter name="AD_Org_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="DateFrom" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="DateTo" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="IsSOTrx" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[select o.ad_org_id,
	o.value, 
	o.name,
	ah.c_allocationhdr_id,
	ah.documentno,
	ah.datetrx,
	ah.grandtotal,
	pocravailable(ah.c_allocationhdr_id) as open,
	coalesce(ip.c_invoice_id, ic.c_invoice_id) as c_invoice_id,
	invoiceopen(coalesce(ip.c_invoice_id, ic.c_invoice_id),0) as invoiceopen,
	coalesce(ip.documentno, ic.documentno) as invoice_documentno,
	coalesce(ip.dateinvoiced, ic.dateinvoiced) as dateinvoiced,
	coalesce(ip.grandtotal, ic.grandtotal) as invoice_grandtotal,
	sum(coalesce(alp.amount, alc.amount)) as amount
	
from c_allocationhdr ah
inner join c_allocationline al on al.c_allocationhdr_id = ah.c_allocationhdr_id
inner join ad_org o on o.ad_org_id = ah.ad_org_id

left join c_allocationline alp on (alp.c_payment_id = al.c_payment_id and alp.c_allocationhdr_id <> al.c_allocationhdr_id)
left join c_invoice ip on ip.c_invoice_id = alp.c_invoice_id
left join c_allocationhdr ahp on ahp.c_allocationhdr_id = alp.c_allocationhdr_id

left join c_allocationline alc on (alc.c_cashline_id = al.c_cashline_id and alc.c_allocationhdr_id <> al.c_allocationhdr_id)
left join c_invoice ic on ic.c_invoice_id = alc.c_invoice_id
left join c_allocationhdr ahc on ahc.c_allocationhdr_id = alc.c_allocationhdr_id

where  ah.ad_client_id = $P{AD_CLIENT_ID}
	and ($P{AD_Org_ID} is null OR $P{AD_Org_ID} = 0 OR ah.ad_org_id = $P{AD_Org_ID})
	and ($P{DateFrom}::date is null OR $P{DateFrom}::date <= ah.datetrx::date)
	and ($P{DateTo}::date is null OR $P{DateTo}::date >= ah.datetrx::date)
	and (CASE WHEN $P{IsSOTrx} = 'Y' THEN ah.allocationtype = 'RCA' ELSE ah.allocationtype = 'OPA' END)
	and (ahp.c_allocationhdr_id is null OR ahp.docstatus in ('CO','CL'))
	and (ahc.c_allocationhdr_id is null OR ahc.docstatus in ('CO','CL'))

group by o.ad_org_id,
	o.value, 
	o.name,
	ah.c_allocationhdr_id,
	ah.documentno,
	ah.datetrx,
	ah.grandtotal,
	ip.c_invoice_id, 
	ic.c_invoice_id,
	ip.documentno, 
	ic.documentno,
	ip.dateinvoiced, 
	ic.dateinvoiced,
	ip.grandtotal, 
	ic.grandtotal
	
order by ah.c_allocationhdr_id,
	ah.documentno,
	ah.datetrx,
	coalesce(ip.documentno, ic.documentno)]]></queryString>

