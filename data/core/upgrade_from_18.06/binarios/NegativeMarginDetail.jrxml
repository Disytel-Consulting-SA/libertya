	<queryString><![CDATA[select *,
	net_sales - total_cost as margen,
	((case when net_sales = 0 then 0 else (net_sales - total_cost)/net_sales end))::numeric(22,2) as margen_porc
FROM (select o.ad_org_id, 
	o.value as orgvalue, 
	o.name as orgname,
	p.m_product_id,
	p.value as productvalue, 
	p.name as productname,
	i.c_invoice_id,
	dt.name as doctypename,
	dt.signo_issotrx,
	i.documentno,
	i.dateinvoiced, 
	il.c_invoiceline_id,
	il.qtyinvoiced * dt.signo_issotrx as qtyinvoiced,  
	(il.costprice * il.qtyinvoiced)::numeric(22,2) * dt.signo_issotrx as total_cost,
	((CASE WHEN i.istaxincluded = 'Y' THEN linenetamt ELSE linenetamt+taxamt END) - il.documentdiscountamt)::numeric(22,2) * dt.signo_issotrx as total_sales,
	(((linenetamt - il.documentdiscountamt) / (CASE WHEN i.istaxincluded = 'N' THEN 1 ELSE (1 + (rate/100)) END)))::numeric(22,2) * dt.signo_issotrx as net_sales
FROM c_invoice i
JOIN ad_org o ON i.ad_org_id = o.ad_org_id
JOIN c_doctype dt ON dt.c_doctype_id = i.c_doctypetarget_id
JOIN c_invoiceline as il on il.c_invoice_id = i.c_invoice_id
JOIN c_tax as t on t.c_tax_id = il.c_tax_id
JOIN m_product as p on p.m_product_id = il.m_product_id
WHERE i.ad_client_id = $P{AD_CLIENT_ID} 
	AND ($P{AD_Org_ID} is null OR $P{AD_Org_ID} = 0 OR i.ad_org_id = $P{AD_Org_ID}) 
	AND dt.isfiscaldocument = 'Y'::bpchar 
	AND (CASE WHEN $P{DocOption} = 'A' THEN dt.docbasetype IN ('ARI', 'ARC') 
		WHEN $P{DocOption} = 'S' THEN dt.docbasetype = 'ARI' 
		ELSE dt.docbasetype = 'ARC' END)
	AND (dt.doctypekey::text <> ALL (ARRAY['RCI'::text, 'RCR'::text])) 
	AND (i.docstatus = ANY (ARRAY['CL'::bpchar, 'CO'::bpchar, 'VO'::bpchar, 'RE'::bpchar])) 
	AND (dt.isfiscal IS NULL OR dt.isfiscal = 'N'::bpchar OR dt.isfiscal = 'Y'::bpchar AND i.fiscalalreadyprinted = 'Y'::bpchar) 
	AND (dt.transactiontypefrontliva IS NULL OR dt.transactiontypefrontliva = 'S'::bpchar)
	AND ($P{DateFrom}::date is null OR dateinvoiced::date >= $P{DateFrom}::date) 
	AND ($P{DateTo}::date is null OR dateinvoiced::date <= $P{DateTo}::date)
) as t 
where net_sales < total_cost
ORDER BY dateinvoiced, productvalue]]></queryString>

