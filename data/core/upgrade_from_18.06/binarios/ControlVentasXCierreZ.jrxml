	<parameter name="ORG_NAME" isForPrompting="false" class="java.lang.String"/>
	<parameter name="ORG_VALUE" isForPrompting="false" class="java.lang.String"/>
	<parameter name="DateFrom" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="DateTo" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="AD_CLIENT_ID" isForPrompting="false" class="java.lang.Integer"/>
	<queryString><![CDATA[select fiscalclosingdate, puntodeventa, sum(ventas) as ventas, sum(cierrez_df) as cierrez_df, sum(cierrez_nc) as cierrez_nc
from (select fiscalclosingdate::date as fiscalclosingdate, puntodeventa, 0.00 as ventas, sum(fiscaldocumentamt) as cierrez_df, sum(creditnoteamt) as cierrez_nc
from c_controlador_fiscal_closing_info ci
where ad_client_id = $P{AD_CLIENT_ID}
	and ($P{AD_Org_ID} is null or $P{AD_Org_ID} = 0 or $P{AD_Org_ID} = ad_org_id)
	and fiscalclosingtype = 'Z'
	and ($P{DateFrom}::date is null OR fiscalclosingdate::date >= $P{DateFrom}::date)
	and ($P{DateTo}::date is null OR fiscalclosingdate::date <= $P{DateTo}::date)
group by fiscalclosingdate::date, puntodeventa
union all
select i.dateacct::date as fiscalclosingdate, puntodeventa, sum(amount) as ventas, 0.00 as cierrez_df, 0.00 as cierrez_nc
from v_dailysales_invoices_filtered(
    (CASE WHEN $P{AD_Org_ID} is null THEN 0 ELSE $P{AD_Org_ID} END)::integer,
    -1,
    -1,
    $P{DateFrom}::date,
    $P{DateTo}::date,
    null::date,
    null::date,
    false) as ds
join c_invoice i on i.c_invoice_id = ds.c_invoice_id
group by i.dateacct::date, puntodeventa) as t
group by fiscalclosingdate, puntodeventa
order by fiscalclosingdate, puntodeventa]]></queryString>

