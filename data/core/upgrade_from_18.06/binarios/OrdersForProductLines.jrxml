	<parameter name="AD_Org_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="M_Product_Lines_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="C_BPartner_ID" isForPrompting="false" class="java.math.BigDecimal"/>
	<parameter name="AD_CLIENT_ID" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="DateFrom" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="DateTo" isForPrompting="false" class="java.sql.Timestamp"/>
	<parameter name="IsSOTrx" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[select o.c_order_id, o.dateordered, o.documentno, 
	org.ad_org_id, org.value as orgvalue, org.name as orgname,
	bp.c_bpartner_id, bp.value as bpartnervalue, bp.name as bpartnername,
	pl.m_product_lines_id, pl.value as productlinesvalue, pl.name as productlinesname,
	sum(((ol.linenetamt - ol.documentdiscountamt) / (CASE WHEN o.istaxincluded = 'N' THEN 1 ELSE (1 + (t.rate/100)) END)))::numeric(10,2) as netamount,
	sum(((CASE WHEN o.istaxincluded = 'Y' THEN ol.linenetamt ELSE ol.linenetamt * (1 + (t.rate/100)) END) - ol.documentdiscountamt))::numeric(10,2) as totalamount
from c_orderline ol
join c_order o on o.c_order_id = ol.c_order_id
join c_bpartner bp on bp.c_bpartner_id = o.c_bpartner_id
join ad_org org on org.ad_org_id = o.ad_org_id
join m_product p on p.m_product_id = ol.m_product_id
join m_product_category pc on pc.m_product_category_id = p.m_product_category_id
join m_product_gamas pg on pg.m_product_gamas_id = pc.m_product_gamas_id
join m_product_lines as pl on pl.m_product_lines_id = pg.m_product_lines_id
join c_tax t on t.c_tax_id = ol.c_tax_id
where o.ad_client_id = $P{AD_CLIENT_ID} and o.docstatus = 'CO' 
	and ($P{AD_Org_ID} is null or $P{AD_Org_ID} = 0 or org.ad_org_id = $P{AD_Org_ID})
	and ($P{C_BPartner_ID} is null or $P{C_BPartner_ID} = 0 or bp.c_bpartner_id = $P{C_BPartner_ID})
	and ($P{M_Product_Lines_ID} is null or $P{M_Product_Lines_ID} = 0 or pl.m_product_lines_id = $P{M_Product_Lines_ID})
	and ($P{DateFrom}::date is null or o.dateordered::date >= $P{DateFrom}::date)
	and ($P{DateTo}::date is null or o.dateordered::date <= $P{DateTo}::date)
	and (o.issotrx = $P{IsSOTrx})
group by o.c_order_id, o.dateordered, o.documentno, 
	org.ad_org_id, org.value, org.name,
	bp.c_bpartner_id, bp.value, bp.name,
	pl.m_product_lines_id, pl.value, pl.name
order by o.dateordered, o.documentno, pl.value]]></queryString>

